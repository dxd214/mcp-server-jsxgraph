{
  "version": 2,
  "name": "jsxgraph-mcp-server",
  "builds": [
    {
      "src": "build/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/mcp",
      "dest": "/build/index.js"
    },
    {
      "src": "/sse",
      "dest": "/build/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/index.js"
    }
  ],
  "functions": {
    "build/index.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
