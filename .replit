run = "node start-server.js"
entrypoint = "start-server.js"

[env]
PORT = "3000"
NODE_ENV = "production"

[nix]
channel = "stable-23_11"

[deployment]
run = ["sh", "-c", "node start-server.js"]
publicDir = "/"

[packager]
language = "nodejs"

[packager.features]
packageSearch = true
guessImports = true
enabledForHosting = false
